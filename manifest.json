{
	"name": "Wealthsimple Tradingview",
	"description": "Displays the Tradingview chart in Wealthsimple.",
	"version": "0.0.0.1",
	"manifest_version": 2,
	"icons": {
		"128": "assets/logo.png"
	},
	"homepage_url": "https://github.com/amnrst/wealthsimple-tradingview",
	"permissions": [
		"*://*.youtube.com/*"
	],
	"content_scripts": [
		{
			"matches": [
				"*://*.wealthsimple.com/*"
			],
			"js": [
				"content-script.js"
			],
			"run_at": "document_idle"
		}
	]
}
